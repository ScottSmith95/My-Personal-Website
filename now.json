{
	"name": "ScottHSmith.com",
	"version": 2,
	"builds": [
	    {
		    "src": "package.json",
		    "use": "@now/static-build",
		    "config": { "distDir": "/" }
		}
	],
	"routes": [{
		"src": "**/*.@(css|js)",
		"headers": [{
			"key": "Cache-Control",
			"value": "public"
		}]},
		{
		"src": "**/*.@(svg|mp4|mov|jpg|jpeg|gif|png|webp|ico|zip|pdf|ttf|otf|eot|woff|woff2)",
		"headers": [{
			"key": "Cache-Control",
			"value": "public"
		}]
	}]
}
